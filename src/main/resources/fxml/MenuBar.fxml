<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>

<fx:root type="MenuBar" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
	<Menu mnemonicParsing="false" text="File">
		<items>
			<MenuItem fx:id="menuItemOpenFolder" mnemonicParsing="false" text="Open Folder"/>
			<MenuItem fx:id="menuItemNewFile" mnemonicParsing="false" text="New File"/>
			<MenuItem fx:id="menuItemDeleteFile" mnemonicParsing="false" text="Delete File"/>
		</items>
	</Menu>
	<Menu mnemonicParsing="false" text="Edit"/>
	<Menu mnemonicParsing="false" text="Help">
		<items>
			<MenuItem fx:id="menuItemAbout" mnemonicParsing="false" text="About"/>
		</items>
	</Menu>
</fx:root>
